# Configuración del servidor
PORT=5000
NODE_ENV=development

# BigQuery (opcional en desarrollo)
# GOOGLE_KEY_FILE=path/to/your/service-account-key.json
# O usar JSON directo:
# BQ_KEY_JSON={"type":"service_account","project_id":"..."}

# Google Cloud Storage (opcional - para caché persistente)
# GCS_BUCKET=your-bucket-name

# Redis (opcional - para caché distribuido)
# REDIS_URL=redis://localhost:6379

# API URL (para fetchReplays)
API_URL=http://localhost:5000
